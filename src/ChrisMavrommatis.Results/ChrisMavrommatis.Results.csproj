<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net8.0;net9.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>ChrisMavrommatis.Results</PackageId>
		<Title>ChrisMavrommatis.Results</Title>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Authors>Chris Mavrommatis</Authors>
		<PackageProjectUrl>https://github.com/ChrisMavrommatis/ChrisMavrommatis.Results</PackageProjectUrl>
		<RepositoryUrl>https://github.com/ChrisMavrommatis/ChrisMavrommatis.Results</RepositoryUrl>
		<Version>2.0.3</Version>
		<PackageIcon>package-icon.png</PackageIcon>
		<AssemblyName>ChrisMavrommatis.Results</AssemblyName>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<RootNamespace>ChrisMavrommatis.Results</RootNamespace>
		<RepositoryType>git</RepositoryType>
		<Description>A library providing utility types and for handling results in a clean and expressive manner.</Description>
		<PackageTags>result;union;oneof;functional;utility</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\LICENSE" Pack="true" PackagePath="\" />
		<None Include="..\..\README.md" Pack="true" PackagePath="\" />
		<None Include="..\..\package-icon.png" Pack="true" PackagePath="\" />
	</ItemGroup>

</Project>
